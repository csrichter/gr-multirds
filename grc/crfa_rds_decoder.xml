<?xml version="1.0"?>
<block>
  <name>RDS Decoder (cr)</name>
  <key>multirds_rds_decoder</key>
  <category>[multirds]</category>
  <import>import multirds</import>
  <make>multirds.rds_decoder($log, $debug)</make>
	<param>
		<name>Log</name>
		<key>log</key>
		<value>False</value>
		<type>bool</type>
		<option>
			<name>Enable</name>
			<key>True</key>
		</option>
		<option>
			<name>Disable</name>
			<key>False</key>
		</option>
	</param>
	<param>
		<name>Debug</name>
		<key>debug</key>
		<value>False</value>
		<type>bool</type>
		<option>
			<name>Enable</name>
			<key>True</key>
		</option>
		<option>
			<name>Disable</name>
			<key>False</key>
		</option>
	</param>
	<sink>
		<name>in</name>
		<type>byte</type>
	</sink>
	<source>
		<name>out</name>
		<type>message</type>
		<optional>1</optional>
	</source>
</block>
